{
    "manifest_version": 2,
    "name": "Debate Master",
    "version": "1.0",
    "permissions": ["activeTab", "https://icanhazdadjoke.com/"],
    "background": {
        "scripts": ["dist/background/background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["dist/content/content.js"]
        }
    ],
    "browser_action": {
        "default_title": "Debate Master"
    }
}